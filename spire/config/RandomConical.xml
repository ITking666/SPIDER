<?xml version="1.0" ?>

<Configuration>

<Main>
  <title>Reconstruction of single particles from
random conical tilt pairs</title>
  <image>pics/cobweb.gif</image>
  <helpurl>http://www.wadsworth.org/spider_doc/spider/docs/spire/index.html</helpurl>
</Main>

<Locations>
  <location>../batchfiles/Randomconical</location>
</Locations>

<Dialogs>

<dialog>
<name>batch directory</name>
<title>main directory</title>
<dir>hbl</dir>
<help>http://www.wadsworth.org/spider_doc/spider/docs/spire/index.html</help>
  <button>
    <label>convert micrographs to spider format</label>
    <buttontext>b00.fed</buttontext>
    <proc>b00.fed</proc>
  </button>
  <button>
    <label>pick tilt pairs</label>
    <buttontext>jweb</buttontext>
    <proc><prog>java -Xmx300m web/StartWeb</prog></proc>
  </button>
  <button>
    <label>window particles from tilted micrograph</label>
    <buttontext>b01.fed</buttontext>
    <proc>b01.fed</proc>
  </button>
  <button>
    <label>window particles from untilted micrograph</label>
    <buttontext>b02.fed</buttontext>
    <proc>b02.fed</proc>
  </button>
  <button>
    <label>reference-free alignment</label>
    <buttontext>b03.fed</buttontext>
    <proc>b03.fed</proc>
  </button>
  <button>
    <label>re-orient averages</label>
    <buttontext>b04.fed</buttontext>
    <proc>b04.fed</proc>
  </button>
  <button>
    <label>apply alignment parameters to particles</label>
    <buttontext>b05.fed</buttontext>
    <proc>b05.fed</proc>
  </button>
  <button>
    <label>center tilted particles</label>
    <buttontext>b06.fed</buttontext>
    <proc>b06.fed</proc>
  </button>
  <button>
    <label>store Euler angles</label>
    <buttontext>b07.fed</buttontext>
    <proc>b07.fed</proc>
  </button>
  <button>
    <label>multivariate statistical analysis</label>
    <buttontext>b08.fed</buttontext>
    <proc>b08.fed</proc>
  </button>
  <button>
    <label>make reconstituted and importance images</label>
    <buttontext>b09.fed</buttontext>
    <proc>b09.fed</proc>
  </button>
  <button>
    <label>classify</label>
    <buttontext>b10.fed</buttontext>
    <proc>b10.fed</proc>
  </button>
  <button>
    <label>make symmetry files</label>
    <buttontext>b11.fed</buttontext>
    <proc>b11.fed</proc>
  </button>
  <button>
    <label>make 3d reconstructions</label>
    <buttontext>b12.fed</buttontext>
    <proc>b12.fed</proc>
  </button>
  <button>
    <label>montage projections of volumes</label>
    <buttontext>b13.fed</buttontext>
    <proc>b13.fed</proc>
  </button>
  <button>
    <label>align class-volumes</label>
    <buttontext>b14.fed</buttontext>
    <proc>b14.fed</proc>
  </button>
  <button>
    <label>merge volumes</label>
    <buttontext>b15.fed</buttontext>
    <proc>b15.fed</proc>
  </button>
</dialog>
</Dialogs>

<Directories>
  <dir name="hbl">
    b00.fed, b01.fed, b02.fed, b03.fed, b04.fed, b05.fed, b06.fed, b07.fed, b08.fed, b09.fed, b10.fed, b11.fed, b12.fed, b13.fed, b14.fed, b15.fed
  </dir>
  <dir name="doc"></dir>
  <dir name="images"></dir>
  <dir name="micrographs"></dir>
  <dir name="r2d"></dir>
  <dir name="r3d"></dir>
</Directories>
</Configuration>
