@; b62.dat
; Make images from marker doc files
; Images can be made smaller to show faster as a movie
do lb1 i=1,122   ; EDIT marker file number range
x11=i
pp ll
d_rot{***x11}  ; EDIT marker file prefix if necessary
mrk{***x11}
(900,900)      ; EDIT size of images on which markers were picked
(1,2)
(-1)
(1)
(0)
(0)
(1,1)
(0,0)
ip
mrk{***x11}
tlt{***x11}     ; EDIT file prefix of output images 
(507,507)       ; EDIT desired size of output images
lb1
en
