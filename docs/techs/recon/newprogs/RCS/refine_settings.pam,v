[shrange]  = 6        ; Alignment shift (pixels) searched is +- this value
[iter1]    = 1        ; Starting alignment step (iteration)
[iter-end] = 4        ; Ending alignment step   (iteration)

[diam]     = 349      ; Diameter of the structure (A) used in alignment search.
d61 1
a61 1
