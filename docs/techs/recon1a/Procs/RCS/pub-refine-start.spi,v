   VB OFF                   ; File info turned off
d48 1
a48 1
 IF ( [task] == 0 ) THEN    ; Runs main regular angle refinement loop tasks
d75 1
a75 1
 ELSEIF ( [task] == 1 ) THEN          ; Runs main small-angle refinement loop tasks
d98 11
d110 1
@


