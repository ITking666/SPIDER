     @@refine-smangloop([ang-step-sm],[r2],[alignsh],[prj-radius],[iter],[grp]],[pixsiz],[bp-type])
@


1.45
log
@Inlined cloning, changed dirs
@
text
@d13 1
a13 1
 ;         Inlined cloning, changed dirs         ArDean Leith Mar 2016
d15 1
a15 1
 ; PURPOSE: Runs projection loop or main loop on this parallel node   
d25 2
a26 4

 ; qsub -v run="./spider spi/mrc @@pub-refine-start  2 task=0 iter=2 grp=2 rn=2398 pubsub=1" qsub.pbs

; -------------------- END BATCH HEADER ----------------------------------------
d35 1
a35 1
   SET MP                   ; Set OMP processors to all available
d39 1
a39 1
 @@refine-settings([pixsiz],[r2],[alignsh],[prj-radius],[iter1],[iter-end],[sphdecon],[smallang-yn],[qsub],[incore-yn],[prepare-yn],[bp-type])
d41 2
d51 1
a51 1
 ELSEIF ( [task] == 3 ) THEN ; Runs spherical deconvolution  task -------------------
d54 1
a54 1
   @@refine-sphdecon([iter],[sphdecon],[grp])
@


1.44
log
@@@refine-smangloop([ang-step-sm]
@
text
@d13 1
d26 3
a28 1
 ; -------------------- END BATCH HEADER ----------------------------------------
d41 1
a41 1
 @@refine-settings([pixsiz],[r2],[alignsh],[prj-radius],[iter1],[iter-end],[sphdecon],[smallang-yn],[winsiz],[incore-yn],[prepare-yn],[bp-type])
d44 3
a46 1
   [temp_local_dir]         ; Dir name
d48 2
d51 9
a59 1
 IF ( [task] < 2 ) THEN     ; Runs one of two main  alignment loop tasks
d63 24
a86 1
      @@pub-ref-loop-clone([iter],[grp])
a87 3
   ELSE
      ; Redefine [temp_local_dir] to work directory 
      GLO [temp_local_dir] = '[temp_work_dir]'
d107 1
d109 1
a109 3
      ; Copy local disk files back to master disk  
      @@pub-ref-loop-declone([iter],[grp])
   ENDIF
d111 2
d114 4
a117 10
 ELSEIF ( [task] == 2 ) THEN  ; Runs projection task -------------------------

   ; For volume projection in parallel. 
   @@refine-prjloop([prj-radius],[iter],[grp])

 ELSEIF ( [task] == 3 ) THEN  ; Runs spherical deconvolution  task -------------------------

   ; For spherical deconvolution in parallel
   @@refine-sphdecon([iter],[sphdecon],[grp])

@


