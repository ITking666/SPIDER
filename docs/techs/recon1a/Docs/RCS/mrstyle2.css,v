1.7
log
@cosmetic
@
text
@
/* 'project' is used to flag those batch files that should be         */
/* included in the project, for software that reads mr1.html.         */
/* Links should be of the form:                                       */
/* <a class="project" href="./newprogs/refproj.spi">                  */
/* and should only be used in the step-by-step flow of batch files,   */
/* not in introductory or explanatory paragraphs. This ensures that   */
/* the desired procedure files are captured once in the correct order */

a.project    { font-weight      : normal }

h2.top       { color         : #99ccff;
               text-align    : center
             }

h3.heading   { text-align    : center }

p.action     { font-weight   : bold }

p.explain    { margin-left   : 20%  ;
             margin-right    : 20% ;
             text-align      : justify
             }
code.snippet { font-size     : 80%     ; font-weight : bold ;  }

code.output  { color         : #005000 ; font-weight : bold ;  }

font.guitool { color         : Peru ; font-weight : bold ; }

font.input   { color:deeppink   ; font-style: italic }

font.output  { color:#007000    ; font-weight: bold  }

ol.choice    { list-style-type  : lower-alpha }

ul.options   { list-style-type  : default }

ul.circles   { list-style-type  : circle }

table.top    { background-color : #000000 ;
               border-color     : #99ccff
             }

table.outs   { valign      : top ;
               cellspacing : 3 ;
               cellpadding : 3 ;
             }
	     
table.heading { background-color : #99ccff ;  }

div.explain  { margin-left   : 20%  ;
               margin-right  : 20% ;
               text-align    : justify        
	     }

div.hidediv  { display : none  ; }

div.left     {
             margin-left    : 0px;
             //padding      : 5px 10px 5px 12px; 
             //font-size    : 40px;
             //font-weight  : bold;
             //font-family    : sans-serif;
             }

div.right    {
             margin-left    : 12px;
             padding        : 20px 3px 20px 8px;
             vertical-align : top;
             line-height    : 120%;
             }
	     
span.nobreak { white-space: nowrap;}

	     
/* text-align : center; */
/* text-decoration: underline} */
@


