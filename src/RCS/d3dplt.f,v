      SCALFC = 1
1002  CALL RDPRM2S(SCALFC,-1,NLOOP,ILOOP,
d84 1
a84 1
      SCLMAX = SCLMAX*SCALFC
d93 7
a99 7
      NX    = 1 + NSAM2
      NXG   = NX + NSAM2
      NG    = NXG + NRES2 - 40
      NXH   = NG + NRES2 - 40
