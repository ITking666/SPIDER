      ANSW(:1) = 'N'
      CALL RDPRMC(ANSW,NC,.TRUE.,
     &   'DO YOU WANT A COORDINATE SYSTEM? (N/Y)',NULL,IRTFLG)
      IF (IRTFLG .EQ. -1) GOTO 1022
      IF (ANSW(:1) .EQ. 'Y') THEN
         ANSW(:1) = 'N'
         WRITE(NOUT,*) ' SORRY, COORDINATE SYTEM NOT AVAILABLE YET'
