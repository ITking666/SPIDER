401   DISP   = 'UNK'
      MAXCNT = 4500
      IF (MAXCNT .GE. IMAXCT) MAXCNT = IMAXCT - 1

C     OPEN A CNT FILE FOR OUTPUT
      CALL CNFILE(CSFILE,NLETC,'CONTOUR',DISP,EXTEN,LUNCS,IRT)
      IF (IRT .EQ. -1) RETURN
       
d79 2
