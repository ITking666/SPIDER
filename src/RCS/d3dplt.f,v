     1    BUF(NH),NGOP,NRES,NSAM,NROW/NSKIP,XLNTH,YLNTH,XMINT,DELTAX,
     2    FMIN,DELTAY,XMAXT,LUNCS)
d171 2
a172 2
          CALL ERRT(6,'D3DPLT',NE)
          GO TO 20
d178 4
