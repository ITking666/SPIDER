	SUBROUTINE  SH180_2(X,Y,LSD2,NSAM,NROW,SX,SY)
d31 20
a50 20
	COMPLEX  X(LSD2,NROW),Y(LSD2,NROW)
	COMPLEX  R
	DOUBLE PRECISION  PI2

	IF (SX.EQ.0.0  .AND.  SY.EQ.0.0)  THEN
	   CALL  COP(X,Y,2*LSD2*NROW)
	   RETURN
	ENDIF

C	INS=1
C	CALL  FMRS_2(X,NSAM,NROW,INS)
