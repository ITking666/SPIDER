1.2
log
@nosave
@
text
@d1 1
a1 1
	SUBROUTINE  SH180_2(X,Y,NSAM,NROW,WRK,SX,SY)
a5 1
C  WRK(MAX0(NSAM,NROW))
d7 1
a7 1
	DIMENSION  X(NSAM,NROW),Y(NSAM,NROW),WRK(*)
d11 4
d16 1
a16 1
C	CALL  Fmr_2(X,NSAM,NROW,WRK,INS)
d23 13
a35 122
c
	IF(SX.EQ.0.0  .AND.  SY.EQ.0.0)  THEN
C
	DO  501  I=2,NSAM/2
	KI=2*I-1
	DO  501  J=1,NROW
	Y(KI,J)=X(KI,J)
	Y(KI+1,J)=-X(KI+1,J)
501	CONTINUE
C
	IF(MOD(NSAM,2).EQ.0)  THEN
	DO  502  I=1,2
   	DO  503  J=2,NROW/2
	KJ=2*J-1
	Y(I,KJ)=X(I,KJ)
	Y(I,KJ+1)=-X(I,KJ+1)
503	CONTINUE
	IF(MOD(NROW,2).EQ.1)  THEN
	Y(I,NROW)=X(I,NROW)
	Y(I,2)=-X(I,2)
	ENDIF
502	CONTINUE
	Y(2,1)=X(2,1)
	IF(MOD(NROW,2).EQ.0)  THEN
	Y(1,2)=X(1,2)
	Y(2,2)=X(2,2)
	ENDIF
C
	ELSE	
C
 	DO  601  J=2,NROW/2
	KJ=2*J-1
	Y(1,KJ)=X(1,KJ)
	Y(1,KJ+1)=-X(1,KJ+1)
601 	CONTINUE
	IF(MOD(NROW,2).EQ.0)  THEN
	Y(1,2)=X(1,2)
	ELSE
	Y(1,NROW)=X(1,NROW)
	Y(1,2)=-X(1,2)
	ENDIF
	DO  602  J=1,NROW
	Y(NSAM,J)=X(NSAM,J)
	Y(2,J)=-X(2,J)
602 	CONTINUE
	ENDIF
	Y(1,1)=X(1,1)
c
	goto  1010
	ENDIF
C
	DO  201  I=2,NSAM/2
	IX=I-1
	KI=2*I-1
	ARGX=PI2*SX*IX/FLOAT(NSAM)
	DO  201  J=1,NROW
	IY=J-1
	IF(IY.GT.NROW/2)  IY=IY-NROW
	ARGY=PI2*SY*IY/FLOAT(NROW)
	R=CMPLX(X(KI,J),-X(KI+1,J))*CMPLX(COS(ARGX+ARGY),SIN(ARGX+ARGY))
	Y(KI,J)=REAL(R)
	Y(KI+1,J)=AIMAG(R)
201	CONTINUE
C
	IF(MOD(NSAM,2).EQ.0)  THEN
	DO  202  I=1,2
	IX=(I-1)*(NSAM/2)
	ARGX=PI2*SX*IX/FLOAT(NSAM)
   	DO  203  J=2,NROW/2
	IY=J-1
	KJ=2*J-1
	ARGY=PI2*SY*IY/FLOAT(NROW)
	R=CMPLX(X(I,KJ),-X(I,KJ+1))*CMPLX(COS(ARGX+ARGY),SIN(ARGX+ARGY))
	Y(I,KJ)=REAL(R)
	Y(I,KJ+1)=AIMAG(R)
203	CONTINUE
	IF(MOD(NROW,2).EQ.1)  THEN
	ARGY=PI2*SY*int(NROW/2)/FLOAT(NROW)
	R=CMPLX(X(I,NROW),-X(I,2))*CMPLX(COS(ARGX+ARGY),SIN(ARGX+ARGY))
	Y(I,NROW)=REAL(R)
	Y(I,2)=AIMAG(R)
	ENDIF
202	CONTINUE
	Y(2,1)=X(2,1)*COS(PI2*SX/2)
	IF(MOD(NROW,2).EQ.0)  THEN
	Y(1,2)=X(1,2)*COS(PI2*SY/2)
	Y(2,2)=X(2,2)*COS(PI2*(SX+SY)/2)
	ENDIF
C
	ELSE	
C
 	DO  301  J=2,NROW/2
	IY=J-1
	KJ=2*J-1
	ARGY=PI2*SY*IY/FLOAT(NROW)
	R=CMPLX(X(1,KJ),-X(1,KJ+1))*CMPLX(COS(ARGY),SIN(ARGY))
	Y(1,KJ)=REAL(R)
	Y(1,KJ+1)=AIMAG(R)
301 	CONTINUE
	IF(MOD(NROW,2).EQ.0)  THEN
	Y(1,2)=X(1,2)*COS(PI2*SY/2)
	ELSE
	ARGY=PI2*SY*int(NROW/2)/FLOAT(NROW)
	R=CMPLX(X(1,NROW),-X(1,2))*CMPLX(COS(ARGY),SIN(ARGY))
	Y(1,NROW)=REAL(R)
	Y(1,2)=AIMAG(R)
	ENDIF
	ARGX=PI2*SX*int(NSAM/2)/FLOAT(NSAM)
	DO  302  J=1,NROW
	IY=J-1
	IF(IY.GT.NROW/2)  IY=IY-NROW
	ARGY=PI2*SY*IY/FLOAT(NROW)
	R=CMPLX(X(NSAM,J),-X(2,J))*CMPLX(COS(ARGX+ARGY),SIN(ARGX+ARGY))
	Y(NSAM,J)=REAL(R)
	Y(2,J)=AIMAG(R)
302 	CONTINUE
	ENDIF
	Y(1,1)=X(1,1)
c
1010	CONTINUE
C1010	INS=-1
C	CALL  Fmr_2(Y,NSAM,NROW,WRK,INS)
@


