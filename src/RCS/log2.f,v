C **********************************************************************
C
C
C $$ LOG2: RETURNS THE SMALLEST POWER BY WHICH 2 HAS TO BE
C          RAISED TO OBTAIN AN INTEGER .GE.M
C 	   ONLY DEFINED FOR M.GE.1 !
C
C      FUNCTION LOG2(M)
C
C--*******************************************************************
C
C
        FUNCTION LOG2(M)
        LOG2 = 0
        I=1
        DO  K=1,100000
           IF(I.GE.M) RETURN
           I = I*2
           LOG2 = LOG2+1
        ENDDO
        END
@


