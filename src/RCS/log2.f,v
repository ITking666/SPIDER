@
text
@d25 9
a33 9
      FUNCTION LOG2(M)

       LOG2 = 0
       I = 1
	IF(I.GE.M) RETURN
1      I = I*2
       LOG2 = LOG2+1
       IF(I.LT.M) GOTO 1
       END
@


