C=* along with this program. If not, see <http://www.gnu.org/licenses> *
C=*                                                                    *
C **********************************************************************
C
C  $$ ISDIGI(TCHAR)
C
C     PURPOSE:   TO DETERMINE IF A CHARACTER IS A DIGIT (0...9) OR NOT
C
C--*********************************************************************

      LOGICAL FUNCTION ISDIGI(TCHAR)

      CHARACTER TCHAR  

      IF (TCHAR .GE. '0' .AND. TCHAR .LE. '9') THEN
        ISDIGI = .TRUE.
      ELSE
        ISDIGI = .FALSE.
      ENDIF

      END
