1.3
log
@formula corrected
exp[ -(ak**4*ds1**2*Pi**2)/(16*log(2.0))]
@
text
@d3 1
a3 1
C $$ TF.FOR
d15 1
d17 1
a17 1
C $$ TF:
a18 11
C    CALL TF(B,CS,DZ,LAMBDA,KM,NSAM,Q,DS,IE)
C      B
C      CS
C      DZ
C      LAMBDA
C      KM
C      NSAM
C      Q
C      DS
C      IE
C
d20 1
a20 3
C changed 9/5/94 to include intelligible
C Gaussian parameter jf
C
d22 6
d29 1
d31 1
d43 13
a55 9
	DO 50 K=1,NSAM
	AK=FLOAT(K-1)*DK
	P=AK**3-DZ1*AK
	CH=EXP(AK**4*KAPPA)
         B(K)=(EXP(F*Q1*P**2)*CH)*2*EXP(-ENV1*AK**2)
         IF(IE.EQ.1)GO TO 50
         QQT=2.*PI*(.25*AK**4-.5*DZ1*AK**2)
         B(K)=B(K)*((1.0-WGH)*SIN(QQT)-WGH*COS(QQT))
50	CONTINUE
@


