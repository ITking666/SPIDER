C  PARAMETERS:                                                         *
C                                                                      *
C        0         2         3         4         5         6         7 *
C23456789012345678901234567890123456789012345678901234567890123456789012
C***********************************************************************

	SUBROUTINE  RDSL_P(LUN,X,NSAM,NROW,K)

	DIMENSION  X(NSAM,NROW)

	DO    J=1,NROW
           CALL  REDLIN(LUN,X(1,J),NSAM,(K-1)*NROW+J)
	ENDDO
	END
@


