	SUBROUTINE   DOMIN3_S(SQ,NMAT,IPCUBE,NN,TMIN)
d28 1
a28 1
	DIMENSION    SQ(NMAT),IPCUBE(5,NN)
d33 1
a33 1
		SQ(J)=AMAX1(SQ(J),TMIN)
d36 1
a36 1
	END
@


1.5
log
@a
@
text
@a0 2
	subroutine   domin3_s(sq,nmat,ipcube,nn,tmin)
	dimension    sq(nmat),ipcube(5,nn)
d2 27
d30 6
a35 6
		do    i=1,nn
		do    j=ipcube(1,i),ipcube(2,i)
		sq(j)=amax1(sq(j),tmin)
		enddo
		enddo
	end
@


