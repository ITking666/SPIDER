1.2
log
@*** empty log message ***
@
text
@d1 1
a1 1
	SUBROUTINE CLSS(M1,MD,NMAX,TMEAN,D,AR,E,JV
d3 2
a4 1
        REAL*8 TMEAN(9),D(9,2),AR(3,2),E(3),V(10),VV(10),X(2)
d6 2
a7 1
	DIMENSION VART(MVAR)
d9 9
a17 9
	V(1)=GR
C	V(2)=VARI
C	V(3)=SKEW
C	V(4)=AKURT
C	V(5)=ENTP
C	V(6)=AVAV
C	V(7)=AVVR
C	V(8)=SDAV
C	V(9)=SDVR
d19 3
a21 3
	DO LKS =1,MVAR
	V(LKS+1)=VART(LKS)
	END DO
d23 1
a23 1
	V(10)=V2
d25 4
a28 4
	DO KL=1,M1
	JK=JV(KL)
	VV(KL)=V(JK)
	ENDDO
d46 1
a46 1
       ZMIN=Z
d48 1
a48 1
1      CONTINUE
a49 1
	RETURN
@


1.1
log
@automatic particle picking
@
text
@d23 1
a23 1
	DO 60 KL=1,M1
d25 2
a26 1
60	VV(KL)=V(JK)
d29 4
a32 3
       DO 2 J=1,MD
2      X(J)=0.
       DO 3 J=1,M1
d34 4
a37 2
       DO 3 K=1,MD
3      X(K)=X(K)+Z*D(J,K)
d39 3
a41 2
       DO 4 K=1,MD
4      Z=Z+(X(K)-AR(II,K))**2
@
