a23 1
C                INDEXTOREG CONTENTS OF PARAM(LOOPREGT)        (RET.)
d27 1
a27 1
	INTEGER FUNCTION INDEXTOREG(TCHAR,GETIT,LOOPREGT)
d29 1
a29 1
        CHARACTER (LEN=1) :: TCHAR,TCHART
d31 1
a32 3
        TCHART = TCHAR
        CALL SSUPCAS(TCHART)

d39 1
a39 1
           INDEXTOREG = FINDEXTOREG
d41 1
a41 1
           INDEXTOREG = 0
@


