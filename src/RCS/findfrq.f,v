@a
@
text
@a0 1
	subroutine  findfrq(nx,ny,nz,NDIM,NROW,NSLICE,IX,IY,IZ)
d2 31
a32 6
	IF(nX.EQ.1) THEN
	ix=0
		IF(nY.EQ.1)  THEN
		iy=0
			IF(nZ.EQ.1)  THEN
			iz=0
d34 2
a35 2
			ELSEIF(nZ.EQ.2)  THEN
			iz=nslice/2
d38 1
a38 1
			iz=(nz-1)/2
d40 4
a43 4
		ELSEIF(nY.EQ.2)  THEN
		iy=nrow/2
			IF(nZ.EQ.1)  THEN
			iz=0
d45 2
a46 2
			ELSEIF(nZ.EQ.2)  THEN
			iz=nslice/2
d49 1
a49 1
			iz=(nz-1)/2
d52 3
a54 3
		iy=(ny-1)/2
		iz=nz-1
		IF(IZ.gt.nslice/2)  iz=iz-NSLICE
d57 7
a63 7
	ELSEIF(nX.EQ.2)  THEN
	ix=ndim/2
		IF(nY.EQ.1)  THEN
		iy=0
			IF(nZ.EQ.1)  THEN
			iz=0
			ELSEIF(nz.eq.2)  THEN
d66 1
a66 1
			iz=(nz-1)/2
d69 1
a69 1
		ELSEIF(ny.eq.2)  THEN
d71 1
a71 1
			IF(nz.eq.1)  THEN
d73 1
a73 1
			ELSEIF(nz.eq.2)  THEN
d76 1
a76 1
			iz=(nz-1)/2
d80 3
a82 3
		iy=(ny-1)/2
		iz=nz-1
		IF(IZ.gt.nslice/2)  iz=iz-NSLICE
d86 5
a90 5
	ix=(nx-1)/2
	iy=ny-1
	if(iy.gt.nrow/2)  iy=iy-nrow
	iz=nz-1
	IF(IZ.gt.nslice/2)  iz=iz-NSLICE
@


