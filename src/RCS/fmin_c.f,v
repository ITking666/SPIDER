C        0         2         3         4         5         6         7 *
C23456789012345678901234567890123456789012345678901234567890123456789012
C***********************************************************************

	SUBROUTINE  FMIN_C(X,N,XMIN)

	DIMENSION   X(N)

	XMIN=X(1)
	DO    I=2,N
	   XMIN=AMIN1(XMIN,X(I))
	ENDDO
	END
@


