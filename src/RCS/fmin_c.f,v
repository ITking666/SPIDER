1.4
log
@a
@
text
@a0 2
	subroutine  fmin_c(x,n,xmin)
	dimension   x(n)
d2 32
a33 5
	xmin=x(1)
	do    i=2,n
	xmin=amin1(xmin,x(i))
	enddo
	end
@


