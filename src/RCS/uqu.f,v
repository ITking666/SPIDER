@GPL_2010
@
text
@d28 1
d30 1
a30 2
C                                                                      *
C  PURPOSE:                                                            *
a33 3
C IMAGE_PROCESSING_ROUTINE                                             *
C                                                                      *
C        0         2         3         4         5         6         7 *
d37 3
a39 1
        SUBROUTINE  UQU
d42 12
a53 10
        DIMENSION  P(N+1,N),Y(N+1),PR(N),PRR(N),PBAR(N),AK(N)
        COMMON /ITERU/  ITER
        COMMON  /PARM/  NT,PIT(4)
        EXTERNAL  FCNQ

        DATA  PI/3.1415926/

        ITER=0
        DO    I=1,3
           P(2,I)=PIT(I)/180.0*PI
d57 10
a66 10
        BRACKET=0.34906585
        P(1,1)=P(2,1)-BRACKET
        P(1,2)=P(2,2)+BRACKET
        P(1,3)=P(2,3)+BRACKET
        P(3,1)=P(2,1)+BRACKET
        P(3,2)=P(2,2)+BRACKET
        P(3,3)=P(2,3)-BRACKET
        P(4,1)=P(2,1)-BRACKET
        P(4,2)=P(2,2)+BRACKET
        P(4,3)=P(2,3)-BRACKET
d69 1
a69 1
        DO    J=1,4
d71 1
a71 1
              AK(I)=P(J,I)
d73 1
a73 1
           Y(J)=FCNQ(AK)
d76 3
a78 3
        EPS=0.0001
        ITRMAX=300      
        CALL  AMOEBA(P,Y,N,EPS,FCNQ,ITER,ITRMAX,PR,PRR,PBAR)
d80 2
a81 2
        DO    I=1,3
           PIT(I)=P(4,I)*180.0/PI
d84 2
a85 5
        PIT(4)=1.0-Y(4)
        WRITE(NT,1020)  (PIT(L),L=1,4)
1020    FORMAT(' THE EULERIAN ANGLES FOUND BY MINIMIZATION PROCEDURE',/,
     &  '     PHI,  THETA,  PSI  AND  FUNCTION VALUE',/,
     &  3(1X,F9.4),1X,F10.7)
@


