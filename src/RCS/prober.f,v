1.4
log
@SDER replace by ORCDF
@
text
@d26 1
a26 1
	DO 10 I=1,NROW
d28 1
a28 1
	DO 20 J=1,NSAM
d36 1
a36 1
20	CONTINUE
d38 1
a38 1
10	CONTINUE
@


1.3
log
@new sder
@
text
@d34 1
a34 1
	BUF(J)=SDER(ZALPHA)
@


1.2
log
@nosave, negative values count
@
text
@a22 1
	DATA EPS/0.000001/,IDEV/0/
d25 1
a25 1
	AN1=A*SQRT(FLOAT(N))
d34 1
a34 1
	BUF(J)=2.-2.*SDER(ZALPHA,EPS,N1,SDER1,IDEV)
@


