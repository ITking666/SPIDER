C **********************************************************************
C
C FLUSHFILE(FUNIT)
C
C **********************************************************************

      SUBROUTINE FLUSHFILE(FUNIT)

      INTEGER FUNIT

#if defined (SP_NT) || defined(SP_GFORTRAN) 
        CALL flush(FUNIT)
#else
#if defined (SP_IBMSP3) 
C       AIX
        CALL flush_(FUNIT)
#else
C       IRIX & GNU-Linux
        CALL flush(FUNIT,IRET)
#endif
#endif

      RETURN
      END
@


