        SUBROUTINE  PRP(AA,BB,W,T,M,M1)

        IMPLICIT REAL*8 (A-H,O-Z)
        IMPLICIT INTEGER*2 (I-N)
       DIMENSION  AA(M1,M1),BB(M1,M1),W(M,M),T(M,M)

        DO    I=1,M1
        DO    J=1,M1
        AA(I,J)=W(I,J)
        BB(I,J)=T(I,J)
	ENDDO
	ENDDO
        END
