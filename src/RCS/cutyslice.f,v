        SUBROUTINE CUTYSLICE(VOL,YSLICE,NSAM,NROW,NSLICE,NSAM2,IPOS)
        
        DIMENSION VOL(NSAM,NROW,NSLICE),YSLICE(NSAM2,NROW)
C       PROGRAM TO CUT Z-SLICE OUT OF A 3D RADON OR RADON FOURIER FILE.
C       THE LINE CAN BE TRUNCATED AT NSAM2.

        DO I=1,NSLICE
           DO K=1,NSAM2
              YSLICE(K,I) = VOL(K,IPOS,I)
           ENDDO
        ENDDO
        RETURN
        END
@


