   92       FORMAT(' ERROR, FIRST INPUT RANGE: (',I6,'....',I6,')',/)
d68 2
a69 1
        IF (NVAL .GE. 2 .AND. (NUM2 .LT. ILOW2 .OR. NUM2 .GT. IHI2)) 
d72 1
a72 1
   97       FORMAT(' ERROR, SECOND INPUT RANGE: (',I6,'...',I6,')',/)
a76 1
        RETURN
