	DO  701  J=NDIM,1,-1
 701	POL(NDIM-J+1)=POLTMP(J)
d49 3
a51 2
       DO  1  K=1,J
 1     FATT=FATT*FLOAT(K)
d53 3
a55 2
       DO  3  J=2,NDIM
 3     POL(J)=2.0*POL(J)
d60 3
a62 3
       DO  4  I=1,NDIM
       DO  4  J=1,NDIM
	DO  4  K=1,NDIM
d64 7
a70 3
 4     ROT(I,J,K)=ZERO
       DO  5  I=1,NDIM
       DO  5  K=1,I
d73 1
a73 1
       DO  5  L=1,K1              
d76 1
a76 1
       DO  5  M=1,K2              
d79 1
a79 1
       DO  5  P=1,K3              
d82 1
a82 1
       DO  5  Q=1,K4              
d85 1
a85 1
       DO  5  R=1,K5              
d88 1
a88 1
       DO  5  S=1,K6              
d95 9
a103 1
 5     AMAT(KM1,LM1,MM1)=AMAT(KM1,LM1,MM1)+COEF*COEFF*BB(I)
d105 1
a105 1
       DO  33  I=1,NDIM
d112 2
a113 2
       DO  7  K=2,IM1
       DO  6  L=2,K
d117 4
a120 2
 6     RPOT(L)=IB
 7     RPOT(K+1)=ONE
d128 3
a130 2
 12    DO  13  J=1,JK
 13    RK(J)=RPOT(J)/RN
d134 3
a136 2
       DO  15  J=1,JKK
 15    RK(J)=RPOT(J)/RN
d140 1
a140 1
 18    DO  33  L=1,NDIM
d147 2
a148 2
       DO  20  K=2,LM1
       DO  19  M=2,K
d152 4
a155 2
 19    RPOT(M)=IB
 20    RPOT(K+1)=ONE
d163 3
a165 2
 25    DO  26  J=1,JP
 26    RP(J)=RPOT(J)/RN
d169 3
a171 2
       DO  28  J=1,JPP
 28    RP(J)=RPOT(J)/RN
d176 1
a176 1
 31    DO  33  KZ=1,NDIM
d183 2
a184 2
       DO  40  K=2,KM1
       DO  59  M=2,K
d188 4
a191 2
 59    RPOT(M)=IB
 40    RPOT(K+1)=ONE
d199 3
a201 2
 45    DO  46  J=1,JZ
 46    RZ(J)=RPOT(J)/RN
d205 3
a207 2
       DO  48  J=1,JZZ
 48    RZ(J)=RPOT(J)/RN
d211 1
a211 1
 51    DO  32  J=1,JJK
d213 1
a213 1
       DO  32  K=1,JJP
d215 1
a215 1
       DO  32  JZZZ=1,JJZ
d217 10
a226 4
 32    ROT(M,N,MZZZ)=ROT(M,N,MZZZ)+RK(J)*RP(K)*RZ(JZZZ)*AMAT(I,L,KZ)
 33    CONTINUE
	DO  334  I=2,NDIM
	DO  334  J=2,NDIM
d230 3
a232 2
334	CONTINUE
       DO  34  I=2,NDIM
d236 6
a241 3
       DO  34  J=2,NDIM
       DO  34  K=2,NDIM
 34    ROT(I,J,K)=ROT(I,J,K)/8.0
@


