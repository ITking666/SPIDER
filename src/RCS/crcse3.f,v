            SEC = 0.0
            SNO = 0.0
d48 2
d51 20
a70 18
         DO K=1,NSLICE
            KK=K-NSLICE/2-1
            IF(IABS(KK).LE.IR-1) THEN
               DO J=1,NROW
                  KJ=J-NROW/2-1
                  NR1=J+(K-1)*NROW
                  IF(IABS(KJ).LE.IR-1) THEN
                     CALL REDLIN(LUN,BUF,NSAM,NR1)
                     DO I=1,NSAM
                        KI=I-NSAM/2-1
         R=SQRT(FLOAT(KJ*KJ)+FLOAT(KI*KI)+FLOAT(KK*KK))+1.0
                        L=R
                        IF(L.LE.IR-1) THEN
                           XD=R-L
                           SEC(L)=SEC(L)+BUF(I)*(1.0-XD)
                           SEC(L+1)=SEC(L+1)+BUF(I)*XD
                           SNO(L)=SNO(L)+1.0-XD
                           SNO(L+1)=SNO(L+1)+XD
d78 2
a79 2
         DO    I=1,IR
            SEC(I)=SEC(I)/AMAX1(1.0,SNO(I))
@


