        REAL FUNCTION QPHI(PHI,PINC)
C       QUNATISIZE VARIABLE PHI WITH ITS INCREMENT
        S=SIGN(1.,PHI)
        QPHI=S*FLOAT(INT(ABS(PHI)/PINC+0.5))*PINC
        RETURN
        END
