C  PARAMETERS:                                                         *
C                                                                      *
C        0         2         3         4         5         6         7 *
C23456789012345678901234567890123456789012345678901234567890123456789012
C***********************************************************************

	SUBROUTINE  FMAX_C(X,N,XMAX)

	DIMENSION   X(N)

	XMAX=X(1)
	DO    I=2,N
	   XMAX=AMAX1(XMAX,X(I))
	ENDDO
	END
@


