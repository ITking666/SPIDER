1.4
log
@a
@
text
@a0 2
	subroutine  fmax_c(x,n,xmax)
	dimension   x(n)
d2 32
a33 5
	xmax=x(1)
	do    i=2,n
	xmax=amax1(xmax,x(i))
	enddo
	end
@


