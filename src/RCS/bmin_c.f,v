1.4
log
@a
@
text
@a0 2
	subroutine  bmin_c(bcke,nmat,ipcube,nn,bmin)
	dimension    bcke(nmat),ipcube(5,nn)
d2 34
a35 7
		bmin=bcke(ipcube(1,1))
		do    i=1,nn
		do    j=ipcube(1,i),ipcube(2,i)
		bmin=amin1(bcke(j),bmin)
		enddo
		enddo
	end
@


