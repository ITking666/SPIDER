C **********************************************************************
C
C COP(A,B,N)
C
C PURPOSE:  COPY A INTO: B,  SIZE: N     (PARALLEL)
C
C NOTE:     OUT OF DATE SHOULD BE ABANDONED
C
C23456789012345678901234567890123456789012345678901234567890123456789012
C--*******************************************************************

        SUBROUTINE COP(A,B,N)

        DIMENSION  A(N),B(N)

c$omp parallel do private(i)
        DO I=1,N
           B(I) = A(I)
        ENDDO

        END
@


