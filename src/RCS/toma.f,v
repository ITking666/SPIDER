@automatic particle picking
@
text
@d16 2
a17 2
	DO 331 I=1,NROW
	DO 221 J=1,NSAM
d20 2
a21 2
221	CONTINUE
331	CONTINUE
d30 1
a30 1
	DO 300 I=1,NROW
d45 1
a45 1
300	CONTINUE
@
