text
@d20 5
a24 3
       DO  990  J=1,NMAX
       DO  990  I=1,90
 990   IH(I,J)=0.0
d39 3
a41 3
       DO 5 I=1,NMAX
       IF(KG.EQ.JG(I)) GOTO 6
 5     CONTINUE
d48 1
a48 1
       DO 60 I=1,M1
d50 2
a51 1
  60   XT(I)=VV(J)
d54 1
a54 1
       DO 2 I=1,M1
d56 2
a57 1
 2     X=X+Z*D(I)
d94 7
a100 6
       DO  8  I=1,90
       DO  8  J=1,NMAX
	ILH=IH(I,J)
       KI=MAX(ILH,KATC)
	KATC=KI
8	CONTINUE
d105 1
a105 1
       DO  10  KI=1,40
d107 4
a110 3
       DO  12  J=1,90
 12    LINE(J)=LX
       DO  14  J=1,KL
d112 3
a114 2
 14    LINE(JK)=IKR
       DO  23  J=1,90
d117 1
a117 1
       DO  22  I=1,NMAX
d123 3
a125 2
 22    CONTINUE
 23    IF(LM.NE.0)  LINE(J)=NAM(LM)
d128 1
a128 1
 10    CONTINUE
d130 4
a133 3
       DO  30  I=1,90
 30    LINE(I)=LX
       DO  31  I=1,NMAX
d135 2
a136 1
 31    LINE(J)=IG(I)
d140 3
a142 2
       DO  701  I=1,NMAX
 701   WRITE(LUN51,102)  IG(I),NAM(I),JG(I),NG(I)
@


