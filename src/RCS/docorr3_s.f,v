text
@d1 1
a1 1
	subroutine  docorr3_s(bcke,bcn,nmat,ipcube,nn,ala,sbq)
a4 1
cc$doacross  local(i,j)
d9 3
a11 1
1		sbq=sbq+bcn(j)*bcn(j)
@


