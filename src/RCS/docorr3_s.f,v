1.7
log
@a
@
text
@a0 2
	subroutine  docorr3_s(bcke,bcn,nmat,ipcube,nn,ala,sbqo)
	dimension    bcke(nmat),bcn(nmat),ipcube(5,nn)
d2 28
a29 1
 		sbq=0.0
d31 10
a40 9
		do    i=1,nn
		do    j=ipcube(1,i),ipcube(2,i)
 		bcke(j)=bcke(j)+ala*bcn(j)
		temp=bcn(j)*bcn(j)
		sbq=sbq+temp
	 	enddo
		enddo
	sbqo=sbq
	end
@


