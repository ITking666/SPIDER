	DO    I=1,N
	   IF(MAP(I))  XMIN=AMIN1(XMIN,X(I))
	ENDDO
	XMINO=XMIN
	END
@


1.6
log
@a
@
text
@a0 3
	subroutine  fmin_q(x,map,n,xmino)
	dimension   x(n)
	logical*1  map(n)
d2 28
d31 5
a35 5
	do    i=1,n
	if(map(i))  xmin=amin1(xmin,x(i))
	enddo
	xmino=xmin
	end
@


