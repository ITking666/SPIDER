C                  NLOOPD   NLOOP (USED IN BATCH)                 (SENT)
C                  ILOOPD   ILOOP (USED IN BATCH)                 (SENT)
d26 1
a26 1
      SUBROUTINE RDPRM(F1,NLOOPD,ILOOPD,PROMPT)
d34 1
a34 1
      CALL RDPRINC(PROMPT,1,.FALSE.,NLOOPD,ILOOPD,F1,F2,F3,IRTFLG)
@


1.4
