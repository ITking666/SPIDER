          FUNCTION  WATE(TEMP,FX,WTX,LBAND,JTYPE)
 
          DIMENSION  FX(5),WTX(5) 

          IF(JTYPE.EQ.2)  GO TO 1 
          WATE=WTX(LBAND) 
          RETURN
 1          IF(FX(LBAND).LT.0.0001)  GO TO 2
          WATE=WTX(LBAND)/TEMP
          RETURN
 2          WATE=WTX(LBAND) 
          RETURN
          END 
@


