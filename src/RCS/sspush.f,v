  103      FORMAT(' UN-ACCEPTABLE CONTOUR # = ',I5,
     &            ' should be between 1 and ',I5)
           RETURN
        ENDIF

        
        ZCOO = ISEC
        ZERO = 0.0
        I3D  = 2
        WRITE(LUNSS,90)ISEC,IPLAN,CLASNM(ICLAS),ICNT,NDATA,I3D,ZCOO
