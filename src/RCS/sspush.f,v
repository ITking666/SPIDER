           WRITE(NOUT,104)
  104      FORMAT(' ***ERROR IN STORING CONTOUR.')
           RETURN
        ELSEIF (NF .EQ. 2) THEN
C           ORIGINAL DATA IN THE FILE NEED TO BE RECOVERED
            CALL CSRECV(LUNCS)
            GOTO 4002
        ENDIF           

C         NOW UPDATE THE CONTROL ARRAY 'CONTOR'
        J = (IPLAN-1)*NSEC*NCNT*NCLAS + (ISEC-1)*NCNT*NCLAS + 
     &      (ICLAS-1)*NCNT + ICNT
