	SUBROUTINE FROMTOQ(STRING,NCHAR,BRAC,KET,ILIST,RLIST,NLIST,NPARG)
d31 5
a35 1
	COMMON/PARAM/ PARAM(103),NSEL(10)
d37 2
a38 2
        CHARACTER *(*) STRING
        CHARACTER      BRAC,KET
d40 2
a41 2
	REAL           RLIST(NPARG)
	INTEGER        ILIST(NPARG)
d51 3
a53 2
	NS = IBRAC + 1
	NE = IKET  - 1
d55 2
a56 1
	CALL CHKSTRQ(STRING(NS:NE),0,'I',NSEL,DUM,NLIST,IER)
d58 5
a62 2
C       NOW CHANGE NCHAR TO CUT OFF SUBSTRING FROM FURTHER PROCESSING
	NCHAR = IBRAC - 1
d64 4
a67 1
	DO 20 I=1,NPARG
a69 1
             NLIST = I
d71 1
d75 1
a75 1
20      CONTINUE
@
