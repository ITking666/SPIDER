@nosave
@
text
@d10 1
a10 1
	do  100  kn=1,nn
d13 1
a13 1
	do  101  l=ipcube(1,kn),ipcube(2,kn)
d19 7
a25 4
	do  202  mk=-lh,lh
	DO  202  MJ=-LH,LH
	DO  202  MI=-LH,LH
202	qt=qt+x(i+mi,j+mj,k+mk)
d28 2
a29 2
 101  continue
100	continue
@


