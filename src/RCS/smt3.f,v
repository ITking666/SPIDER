1.4
log
@*** empty log message ***
@
text
@d1 25
a25 4
	SUBROUTINE  smt3(t,X,Y,NSAM,nslice,ipcube,nn)
	DIMENSION   X(NSAM,nslice,3),Y(NSAM,nslice)
	dimension  ipcube(5,nn)
	data  lh/1/,k/2/
d27 6
a32 3
	d=t/26.0
	tm=1.0-t-d
C
d34 19
a52 15
	do    kn=1,nn
	i=ipcube(3,kn)-1
	j=ipcube(5,kn)
	do    l=ipcube(1,kn),ipcube(2,kn)
	i=i+1
        if(j.eq.1 .or. j.eq.nsam)  then
           y(i,j)=x(i,j,k)
           else
	qt=0.0
	do    mk=-lh,lh
	DO    MJ=-LH,LH
	DO    MI=-LH,LH
	qt=qt+x(i+mi,j+mj,k+mk)
	ENDDO
	ENDDO
d54 1
a54 4
	Y(I,j)=tm*x(i,j,k)+d*qt
        endif
	enddo
	enddo
@


1.3
