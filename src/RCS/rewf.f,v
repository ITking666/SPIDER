C **********************************************************************
C
C   REWF(LUN, NSKIP)
C
C **********************************************************************

        SUBROUTINE REWF(LUN, NSKIP)

        REWIND LUN

        IF (NSKIP .LE. 0) RETURN

        DO  N=1,NSKIP
           READ(LUN,*) IIII
        ENDDO

        END
@


