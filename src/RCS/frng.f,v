d20 4
a23 5
C
C $$ FRNG.FOR
C
         SUBROUTINE  FRNG(CIRC,LCIRC,NUMR,NRING)
         INTEGER  NUMR(3,NRING)
d25 5
a29 4
c$omp parallel do private(i,l)
         DO    I=1,NRING
            L=LOG2(NUMR(3,I))
            CALL  FFTR_Q(CIRC(NUMR(2,I)),L)
d31 2
@


