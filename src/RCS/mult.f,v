C                                                                      *
C  PARAMETERS:                                                         *
C                                                                      *
C        0         2         3         4         5         6         7 *
C23456789012345678901234567890123456789012345678901234567890123456789012
C***********************************************************************

         SUBROUTINE MULT(M0,TRACE,A,W)

        IMPLICIT REAL*8 (A-H,O-Z)
        IMPLICIT INTEGER*2 (I-N)
       DIMENSION A(M0,M0),W(M0,M0)

       M1=M0-1
       X=0.0
       DO  I=1,M1
       DO  J=1,M1
        X=X+A(I,J)*W(J,I)
       ENDDO
       ENDDO
       TRACE=X
       END
@


