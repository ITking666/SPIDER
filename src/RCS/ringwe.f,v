1.11
log
@GPL License fixed
@
text
@d4 1
d6 1
a6 1
C                                                                      *
d9 1
a9 1
C=* Copyright (C) 1985-2005  Health Research Inc.                      *
d33 1
d35 1
a35 2
C                                                                      *
C  PURPOSE:                                                            *
a38 3
C IMAGE_PROCESSING_ROUTINE
C                                                                      *
C        0         2         3         4         5         6         7 *
d42 27
a68 1
	SUBROUTINE  RINGWE(WR,NUMR,NRING,MAXRIN)
d70 4
a73 6
	DIMENSION  WR(NRING)
	INTEGER    NUMR(3,NRING),MAXRIN
	PI=8.0*DATAN(1.0D0)
	DO    I=1,NRING
	   WR(I)=REAL(NUMR(1,I))*PI/REAL(NUMR(3,I))
     &     *REAL(MAXRIN)/REAL(NUMR(3,I))
@


