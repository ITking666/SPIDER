	FUNCTION NRQ(SYMBOL)
d23 1
a23 1

d25 1
a25 1
	IF (SYMBOL .EQ. '+' .OR. SYMBOL .EQ. '-' .OR.
d27 2
a28 4
     &      SYMBOL .EQ. '@@') NRQ = -1

	RETURN
	END
